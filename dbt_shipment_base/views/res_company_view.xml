<odoo>
    <data>


        <record id="view_res_company_shipment_base" model="ir.ui.view">
            <field name="name">res.company.form.dbt_shipment_base.inherit</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
            <data>
                 <xpath expr="//notebook" position="inside">
                    <page name="shipment_config" string="Shipment Configuration">
                        <group>
                            <field name="shippment_picking_type_id"/>
                        </group>
                    </page>
                 </xpath>
            </data>
            </field>
        </record>

    </data>
</odoo>

