<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_dbt_shipment" model="ir.sequence">
            <field name="name">Shipment Sequence</field>
            <field name="code">dbt.shipment</field>
            <field name="prefix">SHIP%(range_day)s</field>
            <field name="padding">5</field>
            <field name="use_date_range">True</field>
            <field eval="1" name="number_increment"/>
        </record>

    </data>
</odoo>
